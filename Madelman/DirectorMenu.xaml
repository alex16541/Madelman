<local:Master x:Class="Madelman.DirectorMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Madelman"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1080"
      Title="MasterMenu" Background="White">

    <Grid>
        <Button x:Name="btnExit" Content="Exit" 
            HorizontalAlignment="Left" 
            Margin="950,640,0,0" 
            VerticalAlignment="Top" 
            Width="75" Click="btnExit_Click"/>
        <Button x:Name="btnMaterials" Content="Materials" HorizontalAlignment="Center" Margin="0,0,0,100" VerticalAlignment="Center" Width="75" Click="btnMaterials_Click"/>
    </Grid>
</local:Master>
