<Page x:Class="Madelman.MaterialsAndFurnitureBase"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Madelman"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1080"
      Title="MaterialsAndFurnitureBase"
      Background="White">
    <Grid>
        <Frame x:Name="Frame" Width="1000" Margin="0,35,0,140"/>
        <Button x:Name="btnBack" Content="Back" HorizontalAlignment="Left" Margin="40,0,0,50" VerticalAlignment="Bottom" Width="75" Click="btnBack_Click"/>
        <Button x:Name="btnMaterials" Content="Materials" HorizontalAlignment="Left" Margin="839,0,0,50" VerticalAlignment="Bottom" Width="75" Click="btnMaterials_Click"/>
        <Button x:Name="btnFurniture" Content="Furniture" HorizontalAlignment="Left" Margin="965,0,0,50" VerticalAlignment="Bottom" Width="75" Click="btnFurniture_Click"/>

    </Grid>
</Page>
